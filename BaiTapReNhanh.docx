                    Console.Write("sáu trăm ");
                    break;
                case 7:
                    Console.Write("bảy trăm ");
                    break;
                case 8:
                    Console.Write("tám trăm ");
                    break;
                case 9:
                    Console.Write("chín trăm ");
                    break;

            }
            switch (dozens)
            {
                case 0:
                    if (units != 0 && hundreds != 0) Console.Write("lẻ ");
