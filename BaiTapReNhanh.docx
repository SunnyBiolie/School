static void Main(string[] args)
        {
            Console.Title = "Chương trình C#";
            Console.InputEncoding = Encoding.Unicode;
            Console.OutputEncoding = Encoding.Unicode;


            Console.Write("Nhập điểm: ");
            var sPoint = Console.ReadLine();
            int iPoint = 0;
            switch (sPoint)
            {
                case "1":
                    iPoint = 1 * 10;
                    break;
                case "2":
                    iPoint = 2 * 10;
                    break;
                case "3":
                    iPoint = 3 * 10;
                    break;
                case "4":
                    iPoint = 4 * 100;
                    break;
                case "5":
                    iPoint = 5 * 100;
                    break;
                case "6":
                    iPoint = 6 * 100;
                    break;
                case "7":
                    iPoint = 7 * 1000;
                    break;
                case "8":
                    iPoint = 8 * 1000;
                    break;
                case "9":
                    iPoint = 9 * 1000;
                    break;
                default: 
                    Console.WriteLine("Điểm bạn nhập không hợp lệ!!");
                    break;
            }
            if (iPoint != 0)
            {
                Console.WriteLine($"Điểm thưởng là: {iPoint}");
            }


            Console.ReadKey();
        }
